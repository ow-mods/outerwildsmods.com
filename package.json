{
	"name": "outerwildsmods-svelte",
	"version": "0.0.1",
	"type": "module",
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.22",
		"@sveltejs/kit": "^1.0.0-next.202",
		"@types/commonmark": "^0.27.5",
		"@types/sharp": "^0.29.4",
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"autoprefixer": "^10.4.0",
		"commonmark": "^0.30.0",
		"cssnano": "^5.0.12",
		"eslint": "^8.4.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"gh-pages": "^3.2.3",
		"globby": "^12.0.2",
		"postcss": "^8.4.5",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.1",
		"sharp": "^0.29.3",
		"svelte": "^3.44.3",
		"svelte-check": "^2.2.10",
		"svelte-markdown": "^0.1.14",
		"svelte-preprocess": "^4.10.0",
		"tailwindcss": "^3.0.2",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build && pnpm",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "gh-pages -t -d build",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"copy404": "cpy build/404/index.html build --rename=404.html",
		"postbuild": "node post-build.mjs"
	}
}
